{#
  /**
   * @file
   * Default template for a field.
   */
  #}
  {%
    set classes = [
    bundle|clean_class ~ '__' ~ field_name_clean|clean_class,
    label_display == 'inline' ? 'd-flex',
    content,
  ]
  %}
  {%
    set title_classes = [
    'field__label',
    'ubicacion',
    'font-weight-bold',
    label_display == 'visually_hidden' ? 'visually-hidden',
  ]
  %}
<div{{attributes.addClass(classes)}}>
  <h3>
    {{ label }} BODY
  </h3>
  <div {{ item.attributes.addClass('field__item') }}>
    {{ item.content }}
  </div>
  {% for item in items %}
    <div {{ item.attributes.addClass('field__item') }}>
      {{ item.content }}
    </div>
  {% endfor %}
</div>
